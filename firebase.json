{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [ {
      "source": "**",
      "run": {
        "serviceId": "thelabdrinking",  // "service name" (from when you deployed the container image)
        "region": "us-central1"     // optional (if omitted, default is us-central1)
      }
    } ]
  }
}
